{
  "include": [
    "src/**/*.ts" /* Include every ts file in source folder */
  ],
  "exclude": [
    "node_modules" /* exclude everything in  node_modules */
  ],
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "outDir": "./build",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true
    
  },
